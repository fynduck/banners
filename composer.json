{
    "name": "fynduck/banners",
    "description": "Banner module (banner, slider, carousel) for laravel framework",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "fynduck",
            "email": "stason.k12@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "5.6.*",
        "php": "7.*"
    },
    "autoload": {
        "psr-4": {
            "Fynduck\\Banners\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fynduck\\Banners\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fynduck\\Banners\\BannersServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^3.6"
    }
}
